aterial/BorderColor\";\nimport Box from \"@mui/material/Box\";\nimport Button from \"@mui/material/Button\";\nimport Typography from \"@mui/material/Typography\";\nimport Modal from \"@mui/material/Modal\";\nimport axios from \"axios\";\n\nconst style = {\n  position: \"absolute\",\n  top: \"50%\",\n  left: \"50%\",\n  transform: \"translate(-50%, -50%)\",\n  width: 400,\n  bgcolor: \"background.paper\",\n  border: \"2px solid #000\",\n  boxShadow: 24,\n  p: 4,\n};\nconst styleDelete = {\n  height: \"40px\",\n  width: \"50px\",\n};\nconst BCard = ({\n  category_now,\n  headline_now,\n  description_now,\n  type_now,\n  current,\n  id,\n  handleDelete,\n}) => {\n  const color = [\"#36d7b7\", \"#0cb1f7\", \"#878fda\"];\n  const handleUpdate = async () => {\n    try {\n      console.log(\"yes entered\");\n      const response = await axios.put(\"/api/modify\", {\n        type,\n        category,\n        headline,\n        description,\n        id,\n      });\n\n      if (response.data.success === true) {\n        alert(\"Content successfully saved\");\n      }\n      setOpen(false);\n    } catch (error) {\n      console.log(error);\n      alert(error);\n    }\n  };\n  const [open, setOpen] = useState(false);\n  const handleOpen = () => setOpen(true);\n  const handleClose = () => setOpen(false);\n  const [type, settype] = useState(type_now);\n  const [category, setcategory] = useState(\"\");\n  const [headline, setheadline] = useState(\"\");\n  const [description, setdescription] = useState(\"\");\n\n  return (\n    <>\n      <div className=\"container\">\n        <div className=\"card\">\n          <div class=\"card\" style={{ backgroundColor: color[current % 3] }}>\n            <div class=\"card-body\">\n              <h5 class=\"card-title\">category : {category_now}</h5>\n              <br />\n              <h6 class=\"card-subtitle mb-2 text-muted\">\n                headline : {headline_now}\n              </h6>\n              <br />\n\n              <p>\n                <ArrowRightIcon />: {description_now}\n              </p>\n            </div>\n            <div className=\"d-flex justify-content-around\">\n              <div\n                onClick={() => {\n                  console.log(id);\n                  handleDelete(id);\n                }}\n                className=\"btn btn-danger\"\n                style={styleDelete}\n              >\n                <DeleteIcon />\n              </div>\n              <div>\n                <div onClick={handleOpen} className=\"btn btn-primary\">\n                  <BorderColorIcon />\n                  {/* update */}\n                </div>\n                <Modal\n                  open={open}\n                  onClose={handleClose}\n                  aria-labelledby=\"modal-modal-title\"\n                  aria-describedby=\"modal-modal-description\"\n                >\n                  <Box sx={style}>\n                    <Typography\n                      id=\"modal-modal-title\"\n                      variant=\"h6\"\n                      component=\"h2\"\n                    >\n                      Update Content\n                    </Typography>\n                    <Typography id=\"modal-modal-description\" sx={{ mt: 2 }}>\n                      <form>\n                        <div class=\"form-group\">\n                          <label for=\"exampleInputEmail1\">Category</label>\n                          <input\n                            value={category}\n                            onChange={(e) => setcategory(e.target.value)}\n                            type=\"email\"\n                            class=\"form-control\"\n                            placeholder=\"Enter category\"\n                          />\n                        </div>\n                        <div class=\"form-group\">\n                          <label for=\"exampleInputPassword1\">Headline</label>\n                          <input\n                            value={headline}\n                            onChange={(e) => setheadline(e.target.value)}\n                            type=\"text\"\n                            class=\"form-control\"\n                            placeholder=\"Enter title\"\n                          />\n                        </div>\n                        <div class=\"form-group\">\n                          <label for=\"exampleInputPassword1\">Description</label>\n                          <textarea\n                            value={description}\n                            onChange={(e) => setdescription(e.target.value)}\n                            type=\"text\"\n                            class=\"form-control\"\n                            placeholder=\"Enter description\"\n                          />\n                        </div>\n\n                        <button\n                          onClick={() => handleUpdate()}\n                          type=\"button\"\n                          class=\"btn mt-2 btn-primary\"\n                        >\n                          Update\n                        </button>\n                      </form>\n                    </Typography>\n                  </Box>\n                </Modal>\n              </div>\n            </div>\n          </div>\n        </div>\n      </div>\n    </>\n  );\n};\n\nexport default BCard;\n",{"ruleId":"98","severity":1,"message":"99","line":6,"column":8,"nodeType":"100","messageId":"101","endLine":6,"endColumn":14},{"ruleId":"98","severity":1,"message":"102","line":59,"column":16,"nodeType":"100","messageId":"101","endLine":59,"endColumn":23},"no-unused-vars","'Button' is defined but never used.","Identifier","unusedVar","'settype' is assigned a value but never used."]